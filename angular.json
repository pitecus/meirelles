{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "455bd536-c179-421d-a2c2-464d33345e3e"
  },
  "defaultProject": "resume",
  "newProjectRoot": "projects",
  "projects": {
    "resume": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "extractLicenses": false,
              "namedChunks": true,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "maximumError": "1mb",
                  "maximumWarning": "500kb",
                  "type": "initial"
                },
                {
                  "maximumError": "4kb",
                  "maximumWarning": "2kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "commonChunk": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "index": "src/index.html",
            "inlineStyleLanguage": "scss",
            "main": "src/main.ts",
            "ngswConfigPath": "ngsw-config.json",
            "outputPath": "dist/resume",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "serviceWorker": true,
            "styles": [
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "branch": "master",
            "email": "pitecus@gmail.com",
            "name": "Leo Meirelles",
            "repo": "https://github.com/pitecus/pitecus.github.io.git",
            "silent": false
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "resume:build"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "browserTarget": "resume:build:development"
            },
            "production": {
              "browserTarget": "resume:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "codeCoverage": true,
            "inlineStyleLanguage": "scss",
            "karmaConfig": "karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": [
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.spec.json"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    }
  },
  "version": 1
}